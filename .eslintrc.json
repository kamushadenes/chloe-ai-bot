{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": [
    "eslint:recommended",
    "airbnb"
  ],
  "overrides": [
  ],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "require-jsdoc": 0,
    "valid-jsdoc": 0,
    "max-len": 0,
    "no-console": 0,
    "no-continue": 0,
    "no-unused-vars": [
      "error",
      {
        "args": "none"
      }
    ]
  }
}
